{
  "version": "0.5.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "icons"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prebuild": "run-s copy rename transform index",
    "copy": "rimraf .tmp && cpx 'icons/**/*.svg' '.tmp'",
    "rename": "renamer --find '/icon-|mono-|utility-|fill-|outline-|illustrative-|-\\d+px/ig' '.tmp/**/*.svg'",
    "transform": "rimraf src/icons && run-s transform:illustrative transform:all",
    "transform:all": "svgr --out-dir src/icons --typescript --svgo-config=svgo.config.js --ignore-existing -- .tmp",
    "transform:illustrative": "svgr --out-dir src/icons/illustrative --typescript --svgo-config=svgo-illustrative.config.js -- .tmp/illustrative",
    "posttransform": "rimraf .tmp",
    "index": "ts-node --project tsconfig.node.json ./scripts/build-index.ts",
    "postbuild": "rimraf dist/icons/**/*.d.ts",
    "postindex": "tsc",
    "remove-icon-types": "rimraf dist/icons/**/*.d.ts"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "hbf-icons",
  "author": "John Lombardo",
  "module": "dist/hbf-icons.esm.js",
  "size-limit": [
    {
      "path": "dist/hbf-icons.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hbf-icons.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-react": "^7.17.12",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@svgr/cli": "^6.2.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "babel-loader": "^8.2.5",
    "cpx": "^1.5.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "renamer": "^4.0.0",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.8",
    "ts-node": "^10.8.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^3.9.10"
  }
}